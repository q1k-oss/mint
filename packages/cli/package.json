{
  "name": "@q1k-oss/mint-format-cli",
  "version": "1.0.1",
  "description": "ðŸŒ¿ CLI tool for MINT Format - Convert between JSON and MINT",
  "keywords": [
    "mint",
    "format",
    "cli",
    "json",
    "converter"
  ],
  "homepage": "https://github.com/q1k-oss/mint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/q1k-oss/mint.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "MINT Format Contributors",
  "type": "module",
  "bin": {
    "mint": "./dist/index.js"
  },
  "files": [
